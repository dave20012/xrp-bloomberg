      },
      "analytics-worker": {
        "startCommand": "python workers/analytics_worker.py --loop"
      },
      "news-worker": {
        "startCommand": "python workers/news_worker.py --loop"
      },
      "state-worker": {
        "startCommand": "python workers/state_worker.py --loop"
      },
      "geometry-worker": {
        "startCommand": "python workers/geometry_worker.py --loop"
      }
    }
  }
}
